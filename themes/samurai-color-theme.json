{
	"tokenColors": [
		{
			"settings": {
				"background": "#12171B",
				"foreground": "#CBD8EB",
				"selectionForeground": "#ffffff",
				"findHighlight": "#DFCF98",
				"findHighlightForeground": "#000000",
				"selectionBorder": "#000000",
				"activeGuide": "#9d550fb0",
				"bracketsForeground": "#FFFFFF",
				"bracketsOptions": "underline",
				"bracketContentsForeground": "#FF9F60",
				"bracketContentsOptions": "underline",
				"tagsOptions": "stippled_underline"
			}
		},
		{
			"name": "Source base background",
			"scope": [
				"text",
				"source"
			],
			"settings": {
				"background": "#14181E"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#556552",
				"background": "#5565520C"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#A0E4A3",
				"background": "#A0E4A309"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#73B8E1"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#6897E9"
			}
		},
		{
			"name": "Constant characters",
			"scope": "constant.character",
			"settings": {
				"foreground": "#617CC3"
			}
		},
		{
			"name": "Other constant",
			"scope": "constant.other",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Variable Parameter",
			"scope": [
				"variable.parameter",
				"meta.function.arguments variable.other"
			],
			"settings": {
				"foreground": "#A3ABBD"
			}
		},
		{
			"name": "Meta Function Declaration",
			"scope": [
				"meta.function.declaration",
				"punctuation.definition.bracke"
			],
			"settings": {
				"foreground": "#ab3aac"
			}
		},
		{
			"name": "Meta Function Declaration",
			"scope": "variable.function",
			"settings": {
				"foreground": "#ab3aac"
			}
		},
		{
			"name": "Variable Other",
			"scope": "variable.other",
			"settings": {
				"foreground": "#D8DFE7"
			}
		},
		{
			"name": "Variable Other Property",
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#B7BED1"
			}
		},
		{
			"name": "Variable Language",
			"scope": "variable.language",
			"settings": {
				"foreground": "#69A1C8"
			}
		},
		{
			"name": "Keyword Control",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#69A1C8",
				"background": "#69A1C80C"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d05f5f"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"meta.include"
			],
			"settings": {
				"foreground": "#228ECC",
				"background": "#228ECC0C"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#0000000C"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#BBBBBB",
				"background": "#0000000C"
			}
		},
		{
			"name": "Function name",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#55b5ea"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function argument",
			"scope": "meta.property-name support.type.property-name",
			"settings": {
				"foreground": "#8CB5CC"
			}
		},
		{
			"name": "Function argument",
			"scope": "meta.property-value punctuation.separator.key-value",
			"settings": {
				"foreground": "#A3ABBD"
			}
		},
		{
			"name": "Function argument",
			"scope": [
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"foreground": "#8CB5CC"
			}
		},
		{
			"name": "Function argument",
			"scope": [
				"meta.use support.class.builtin",
				"meta.other.inherited-class support.class.builtin"
			],
			"settings": {
				"foreground": "#A3ABBD"
			}
		},
		{
			"name": "PHP Open/Close Tag",
			"scope": [
				"source.php.embedded punctuation.section.embedded.begin",
				"source.php.embedded punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#C92828"
			}
		},
		{
			"name": "PHP Line",
			"scope": [
				"source.php.embedded.line",
				"source.php.embedded.line string"
			],
			"settings": {
				"background": "#C9282819"
			}
		},
		{
			"name": "PHP in-line variable",
			"scope": [
				"source.php.embedded meta.string-contents variable",
				"source.php.embedded meta.string-contents punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#CCCCCC",
				"background": "#C9282819"
			}
		},
		{
			"name": "PHP SQL Line",
			"scope": [
				"source.sql.embedded.php",
				"keyword.other.DML",
				"support.function.aggregate.sql"
			],
			"settings": {
				"foreground": "#CCCCCC",
				"background": "#A0E4A319"
			}
		},
		{
			"name": "PHP SQL Variables",
			"scope": [
				"source.sql.embedded.php string.quoted punctuation",
				"source.sql.embedded.php string.quoted variable",
				"source.sql.embedded.php string.quoted meta"
			],
			"settings": {
				"foreground": "#CCCCCC",
				"background": "#C9282819"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#4E99C9",
				"background": "#4E99C90C"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "CSS Class",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS Embedded",
			"scope": "source.css.embedded.html",
			"settings": {
				"background": "#CFA0E409"
			}
		},
		{
			"name": "JavaScript Embedded",
			"scope": "source.js.embedded.html",
			"settings": {
				"background": "#CFA0E409"
			}
